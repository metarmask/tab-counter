{
	"manifest_version": 2,
	"name": "Tab Counter",
	"description": "Shows the amount of tabs you've created on every new tab.",
	"version": "1.0",
	"icons": {
		"128": "icon.png"
	},
	"chrome_url_overrides": {
		"newtab": "newtab/newtab.html"
	},
	"permissions": ["storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"options_ui": {
		"page": "options/options.html",
		"chrome_style": true
	}
}